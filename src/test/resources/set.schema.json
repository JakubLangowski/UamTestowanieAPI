{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "set"
    ],
    "properties": {
        "set": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "code": {
                    "type": "string"
                },
                "gathererCode": {
                    "type": "string"
                },
                "oldCode": {
                    "type": "string"
                },
                "magicCardsInfoCode": {
                    "type": "string"
                },
                "releaseDate": {
                    "type": "string"
                },
                "border": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "block": {
                    "type": "string"
                },
                "onlineOnly": {
                    "type": "boolean"
                },
                "booster": {
                    "type": "array",
                    "additionalItems": true,
                    "items": {
                        "anyOf": [
                            {
                                "type": "array",
                                "additionalItems": true,
                                "items": {
                                    "anyOf": [
                                        {
                                            "type": "string"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "string"
                            }
                        ]
                    }
                }
            }
        }
    }
}